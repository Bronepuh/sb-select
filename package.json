{
  "name": "sb-select",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "local:build": "yarn build:client && yarn build:server",
    "local:up": "concurrently -k -n SERVER,CLIENT \"ts-node-dev --project tsconfig.server.json --respawn --transpile-only server/index.ts\" \"webpack serve --mode development --port 3005\"",
    "prod:build": "yarn build:client && yarn build:server",
    "prod:up": "pm2 reload sb-select || pm2 start ecosystem.config.cjs",
    "build:client": "webpack --mode production",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "tsc -p tsconfig.json --noEmit && tsc -p tsconfig.server.json --noEmit"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
